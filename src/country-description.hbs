{{#each this}}
<div class="description-box">
    <p class="title">{{name}}</p>
    <p class="desc">Capital: {{capital}}</p>
    <p class="desc">Population: {{population}}</p>
    <p class="desc">Languages:</p>
    <ul class="lng-list">
    {{#each languages}}
    <li class="desc">{{name}}</li>
    {{/each}}
    </ul>
</div>
<div class="thumb">

    <img {{#each flags}}
    src="{{this}}"
    {{/each}} 
    alt="{{name}} flag" width="300">

    
</div>
{{/each}}